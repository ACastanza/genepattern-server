<!-- map of group/user names to permissions -->
<!-- * indicates anyone has a permission -->
<!-- - indicates no one has a permission -->


<permissionMap>

<permission name="importTask">
	<group name="genepatternTaskCreators"/>
</permission>

<permission name="createTask">
	<group name="genepatternTaskCreators"/>
</permission>

<permission name="createPipeline">
	<group name="*"/>
</permission>

<permission name="arbitrarySql">
	<group name="genepatternAdministrators"/>
</permission>

<permission name="administrateServer">
	<group name="genepatternAdministrators"/>
</permission>


</permissionMap>