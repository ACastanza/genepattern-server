<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.genepattern.server">
  <class name="UserPassword" table="USER_PASSWORD" optimistic-lock="none">
    <id name="id" type="integer" unsaved-value="null" column="USER_PASSWORD_ID">
      <generator class="native"/>
    </id>
    <property name="username" type="string" column="USERNAME"/>
    <property name="password" type="string" column="USER_PASSWORD"/>
  </class>
</hibernate-mapping>
