<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.genepattern.server.auth"> 
  <class name="JobGroup" table="JOB_GROUP"> 
    <composite-id>
      <key-property name="jobNo" column="JOB_NO" />
      <key-property name="groupId" column="GROUP_ID" />
    </composite-id>      
    <property name="permissionFlag" column="PERMISSION_FLAG" />
  </class>
</hibernate-mapping>
