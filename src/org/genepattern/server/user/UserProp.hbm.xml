<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.genepattern.server.user"> 
  <class name="UserProp" table="gp_user_prop" optimistic-lock="none"> 
    <id name="id" type="integer" unsaved-value="null" column="id">
      <generator class="native"/>
    </id>
    <property name="key" type="string" column='"KEY"'/>
    <property name="value" type="string" column="value"/>
    <property name="gpUserId" type="string" column="gp_user_id"/>
  </class>
</hibernate-mapping>
