<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.genepattern.server.webservice.server.dao"> 
  <class name="TaskAccess" table="TASK_ACCESS" optimistic-lock="none"> 
    <id name="accessId" type="big_integer" unsaved-value="null">
      <column name="ACCESS_ID" not-null="true" unique="true" precision="22" scale="0"/>
      <generator class="native"/>
    </id>
    <property name="name" type="string" column="NAME" length="4000"/>
    <property name="description" type="string" column="DESCRIPTION" length="4000"/>
  </class>
</hibernate-mapping>
