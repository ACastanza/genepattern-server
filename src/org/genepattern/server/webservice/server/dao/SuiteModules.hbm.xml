<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.genepattern.server.webservice.server.dao">
  <class name="SuiteModules" table="SUITE_MODULES" optimistic-lock="none">
    <id name="moduleId" type="integer" unsaved-value="null">
      <column name="MODULE_ID" not-null="true" unique="true" precision="10" scale="0"/>
      <generator class="native"/>
    </id>
    <property name="lsid" type="string">
      <column name="LSID" index="IDX_SUITE_MODULES_LSID"/>
    </property>
    <property name="moduleLsid" type="string">
      <column name="MODULE_LSID" index="IDX_MODULES_SUITE_LSID"/>
    </property>
  </class>
</hibernate-mapping>
