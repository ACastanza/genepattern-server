 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>my settings</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>run task</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>job results</from-outcome>
   <to-view-id>/pages/jobResults.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>create suite</from-outcome>
   <to-view-id>/pages/createSuite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>task catalog</from-outcome>
   <to-view-id>/pages/taskCatalog.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>suite catalog</from-outcome>
   <to-view-id>/pages/suiteCatalog.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>commandPrefixAdmin</display-name>
  <from-view-id>/pages/commandPrefixAdmin.xhtml</from-view-id>
  <navigation-case>
   <from-action>*</from-action>
   <from-outcome>*</from-outcome>
   <to-view-id>/pages/commandPrefixAdmin.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/visualizerMemory.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/changePassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/forgotPassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>install</display-name>
  <from-view-id>/pages/taskCatalog.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>install</from-outcome>
   <to-view-id>/pages/installTasks.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>install</display-name>
  <from-view-id>/pages/suiteCatalog.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>install</from-outcome>
   <to-view-id>/pages/installSuites.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <!-- Replace view-handler tag with the tag below to enable trinidad  
 <default-render-kit-id>org.apache.myfaces.trinidad.core</default-render-kit-id>  -->
 </application>
</faces-config>
