  </navigation-case>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>install</display-name>
