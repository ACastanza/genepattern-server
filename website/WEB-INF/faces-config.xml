 <navigation-rule>
  <from-view-id>/pages/serverSettingsSystemMessage.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>redirect</from-outcome>
   <to-view-id>/pages</to-view-id>
  </navigation-case>
 </navigation-rule>
 
 
     
 <navigation-rule>
  <from-view-id>*</from-view-id>
   
  <!-- always redirect after accepting EULA, TODO: this isn't fully implemented yet -->
  <navigation-case>
   <from-outcome>acceptEula</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>

  <navigation-case>
   <from-outcome>registerServer</from-outcome>
   <to-view-id>/pages/registerServer.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>installFrame</from-outcome>
   <to-view-id>/pages/installFrame.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>unregisteredServer</from-outcome>
   <to-view-id>/pages/unregisteredServer.xhtml</to-view-id>
  </navigation-case>
 <navigation-case>
   <from-outcome>contactUs</from-outcome>
   <to-view-id>/pages/contactUs.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>install task results</from-outcome>
   <to-view-id>/pages/installTasks.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>install suite results</from-outcome>
   <to-view-id>/pages/installSuites.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>my settings</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
   <redirect />
  </navigation-case>
  <navigation-case>
   <from-outcome>genomeSpaceLoginFailed</from-outcome>
   <to-view-id>/pages/genomespace/signon.xhtml</to-view-id>   <redirect />
  </navigation-case>
  <navigation-case>
   	<from-outcome>genomeSpaceRegFailed</from-outcome>
    <to-view-id>/pages/genomespace/userRegistration.xhtml</to-view-id>   	<redirect />
  </navigation-case>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>homeNoRedirect</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>unregistered</from-outcome>
   <to-view-id>/pages/unregisteredServer.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>run task</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>job results</from-outcome>
   <to-view-id>/jobResults</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>create suite</from-outcome>
   <to-view-id>/pages/createSuite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>task catalog</from-outcome>
   <to-view-id>/pages/taskCatalog.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>suite catalog</from-outcome>
   <to-view-id>/pages/suiteCatalog.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
