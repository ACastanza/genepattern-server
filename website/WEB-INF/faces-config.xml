<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
 <managed-bean>
  <managed-bean-name>indexPageBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.IndexPageBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.LoginBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pageMessages</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.PageMessages</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>commandPrefixBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.CommandPrefixBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>registrationBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.RegistrationBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>moduleListBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.ModuleListBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>jobResultsBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.JobResultsBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userPrefsBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.UserPrefsBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>changePasswordBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.ChangePasswordBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>forgotPasswordBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.ForgotPasswordBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>runTaskBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.RunTaskBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>taskCatalogBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.TaskCatalogBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>moduleChooserBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.ModuleChooserBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>recentJobsBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.RecentJobsBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>createSuiteBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.CreateSuiteBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>navBarBean</managed-bean-name>
  <managed-bean-class>org.genepattern.server.webapp.jsf.NavBarBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 
 <!--  Beans for example programs -->
<managed-bean>
  <managed-bean-name>navigationMenu</managed-bean-name>
  <managed-bean-class>org.apache.myfaces.examples.misc.NavigationMenu</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
</managed-bean>
 
<managed-bean>
  <managed-bean-name>pollExampleBean</managed-bean-name>
  <managed-bean-class>demo.PollExampleBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>counter</property-name>
   <property-class>java.lang.Integer</property-class>
   <value>0</value>
  </managed-property>
</managed-bean>
 
<managed-bean>
  <managed-bean-name>currentTimeBean</managed-bean-name>
  <managed-bean-class>demo.CurrentTimeBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>

</managed-bean>
   
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
  </navigation-case>
  
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
  </navigation-case>

  <navigation-case>
   <from-outcome>run task</from-outcome>
   <to-view-id>/pages/index.xhtml</to-view-id>
  </navigation-case>
  
  <navigation-case>
   <from-outcome>job results</from-outcome>
   <to-view-id>/pages/jobResults.xhtml</to-view-id>
  </navigation-case>
  
  <navigation-case>
   <from-outcome>create suite</from-outcome>
   <to-view-id>/pages/createSuite.xhtml</to-view-id>
  </navigation-case>
  
  <navigation-case>
   <from-outcome>task catalog</from-outcome>
   <to-view-id>/pages/taskCatalog.xhtml</to-view-id>
  </navigation-case>
  
 </navigation-rule>
 <navigation-rule>
  <display-name>commandPrefixAdmin</display-name>
  <from-view-id>/pages/commandPrefixAdmin.xhtml</from-view-id>
  <navigation-case>
   <from-action>*</from-action>
   <from-outcome>*</from-outcome>
   <to-view-id>/pages/commandPrefixAdmin.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/visualizerMemory.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/changePassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/pages/accountInfo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/forgotPassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <!-- Replace view-handler tag with the tag below to enable trinidad  
 <default-render-kit-id>org.apache.myfaces.trinidad.core</default-render-kit-id>  -->
 </application>
</faces-config>
