
<!-- =================================================================== -->
<!-- Create analysis service jars                                        -->
<!-- =================================================================== -->    

<target name="jar-analysis" depends="init,common,broad-cg,runtask,uisupport">
    <jar jarfile="${build}/analysis.jar">
        <fileset dir="${build}">
            <include name="org/genepattern/analysis/**"/>
             <include name="org/genepattern/client/**"/>
            <include name="org/genepattern/util/**"/>
            <include name="org/genepattern/data/pipeline/**"/>
        </fileset>
    </jar>
   <ant target="post-compile"/>
</target>

<target name="create-analysis-jars"
        depends="jar-analysis">
</target>




 